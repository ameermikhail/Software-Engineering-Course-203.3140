<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <AnchorPane fx:id="rootPane" prefHeight="500.0" prefWidth="600.0">
         <children>
            <MenuBar layoutX="155.0" layoutY="7.0" prefHeight="26.0" prefWidth="258.0">
              <menus>
                <Menu mnemonicParsing="false" text="Homepage">
                  <items>
                    <MenuItem fx:id="student_menu_button" mnemonicParsing="false" onAction="#Handle_menu_clicked" text="Go HomePage" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Exams Grades">
                  <items>
                    <MenuItem fx:id="view_grades_button" mnemonicParsing="false" onAction="#Handle_menu_clicked" text="View" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Exam">
                  <items>
                    <MenuItem fx:id="do_exam_button" mnemonicParsing="false" onAction="#Handle_menu_clicked" text="Do Exam" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
            <Label fx:id="QuestionNumberLabel" layoutX="269.0" layoutY="109.0" text="Question" />
            <Button fx:id="previousButton" layoutX="44.0" layoutY="450.0" minWidth="50.0" onAction="#PreviousButton" prefHeight="37.0" prefWidth="76.0" text="previous" />
            <Button fx:id="NextButton" layoutX="503.0" layoutY="450.0" minWidth="50.0" onAction="#NextButton" prefHeight="37.0" prefWidth="76.0" text="next" />
            <TextArea fx:id="QuestionText" layoutX="30.0" layoutY="156.0" prefHeight="231.0" prefWidth="539.0" />
            <ChoiceBox fx:id="AnswersChoiceBox" layoutX="300.0" layoutY="395.0" prefHeight="26.0" prefWidth="84.0" />
            <Label fx:id="AnswersLabel" layoutX="235.0" layoutY="400.0" text="answer:" />
            <Label fx:id="TimeLeftLabel" layoutX="72.0" layoutY="59.0" prefHeight="18.0" prefWidth="61.0" text="Time Left:" />
            <Label fx:id="TimeLeftValue" layoutX="159.0" layoutY="55.0" prefHeight="26.0" prefWidth="152.0" />
            <Button fx:id="previousButton1" layoutX="255.0" layoutY="448.0" minWidth="50.0" onAction="#SubmitClicked" prefHeight="37.0" prefWidth="76.0" text="submit" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
